class Solution {
public:
    vector<int> twoSum(vector<int>&num, int target) {
       int a = 0;
       int b = 0;
       for(int i = 0; i<num.size()-1 ;i++)
       {
           for(int j = i+1 ; j<num.size();j++)
           {
               if(num[i]+num[j] == target)
               {
                   a = i;
                   b = j;
               }
           }
       }
       return {a,b};

    }
};
